{
  "name": "robuust/craft-mailjet",
  "description": "Mailjet integration for Craft CMS",
  "type": "craft-plugin",
  "keywords": [
    "cms",
    "craftcms",
    "mailjet",
    "mailer",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Robuust",
      "homepage": "https://robuust.digital/"
    }
  ],
  "support": {
    "email": "support@robuust.digital",
    "issues": "https://github.com/robuust/craft-mailjet/issues?state=open",
    "source": "https://github.com/robuust/craft-mailjet",
    "docs": "https://github.com/robuust/craft-mailjet/blob/v1/README.md",
    "rss": "https://github.com/robuust/craft-mailjet/commits/master.atom"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.3.0",
    "craftcms/cms": "^5.0.x-dev",
    "symfony/mailjet-mailer": "^7.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "robuust\\mailjet\\": "src/"
    }
  },
  "extra": {
    "name": "Mailjet Adapter",
    "handle": "mailjet-adapter",
    "documentationUrl": "https://github.com/robuust/craft-mailjet/blob/v1/README.md"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "platform": {
      "php": "8.3.0"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
